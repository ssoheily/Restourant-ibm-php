        DECLARE concat_string1 CHAR;
        DECLARE concat_string2 CHAR ''; 
        DECLARE product_id_response  CHAR;
        DECLARE Feedback  CHAR;
        -- SELECT CAST (miles AS INT)FROM Flights       
		-- SET zeit1=SET var.table[];
        -- SET zeit2 = cast(zeit); 
