CREATE COMPUTE MODULE adminquery
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CREATE FIELD OutputRoot.JSON.Data IDENTITY(JSON.Array)Data;
		SET var.query[] = (SELECT * FROM Database.RESTCHAR1 );
		-- DECLARE count_array INT CARDINALITY(var.query[]);
		-- SET var.table[] = PASSTHRU('SELECT DISTINCT TABLE_ID FROM RESTCHAR1 ORDER BY TABLE_ID ASC');
		DECLARE J INTEGER;
		SET J = 1;
		WHILE J <= 10 DO
				SET J = J + 1;
				SET OutputRoot.JSON.Data.Item[J].BESTELL_ID = var.query[1].BESTELL_ID;	
				SET OutputRoot.JSON.Data.Item[J].PRODUCT_ID = var.query[1].PRODUCT_ID;
         		SET OutputRoot.JSON.Data.Item[J].PRISE = var.query[1].PRISE;
         		SET OutputRoot.JSON.Data.Item[J].NAME_RESTURANT = var.query[1].NAME_RESTURANT;
         	 	SET OutputRoot.JSON.Data.Item[J].TABLE_ID = var.query[1].TABLE_ID;
         		SET OutputRoot.JSON.Data.Item[J].ZEIT = var.query[1].ZEIT;
         		
		END WHILE;
				
		RETURN TRUE;
	END;

END MODULE;