CREATE COMPUTE MODULE MSGResturant_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		 CALL CopyMessageHeaders();
		 CALL CopyEntireMessage();
		
		 DECLARE var REFERENCE TO Environment.Variables;
        DECLARE product_id CHAR;
        DECLARE table_id CHAR;       
        DECLARE namebediener CHAR;
        DECLARE PRICE CHAR '2' ;
       
        SET product_id = OutputRoot.Message.JSON.Data.product_id;
        SET table_id = OutputRoot.Message.JSON.Data.table_id;
        SET namebediener = OutputRoot.Message.JSON.Data.namebediener;
        -- SET var.query[] = (SELECT TABLE_ID FROM Database.RESTAURANT AS T WHERE T.TABLE_ID = table_id );
        -- SET comparif = var.query.TABLE_ID;
        
           --  INSERT INTO Database.RESTAURANT (PRODUCT_ID, PRISE, NAME_RESTURANT,TABLE_ID)
            -- VALUES(product_id ,  PRICE ,namebediener, table_id);
          	
			PASSTHRU ' INSERT INTO Database.RESTAURANT (PRODUCT_ID, PRISE, NAME_RESTURANT,TABLE_ID)
            VALUES('''|| product_id ||''' ,'''|| PRICE ||'''  ,'''|| namebediener||''' ,'''|| table_id ||''' )'; 		
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;