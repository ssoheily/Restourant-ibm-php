CREATE COMPUTE MODULE AdmnRrgister
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
		DECLARE var REFERENCE TO Environment.variables;
		DECLARE username CHAR;
		DECLARE password CHAR;
		DECLARE email CHAR;
		DECLARE result CHAR ;
		-- Header	
		SET OutputRoot.HTTPReplyHeader."Content-Type" = 'text/json';	
		
		-- Message
		
		SET username = InputRoot.JSON.Data.name;	
		SET password = InputRoot.JSON.Data.password;	
		SET email = InputRoot.JSON.Data.email;
		
		INSERT INTO  Database.ADMINTABLE (USERNAME, EMAIL ,PASS_WORD)
         	 VALUES(username,email ,password);
        SET OutputRoot.JSON.Data.result='successful'; 	 
        	RETURN TRUE;
	END;
	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;