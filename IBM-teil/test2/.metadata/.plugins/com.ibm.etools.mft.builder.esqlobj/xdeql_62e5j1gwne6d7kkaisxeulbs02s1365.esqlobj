/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
	  CALL CopyMessageHeaders();
	  SET OutputRoot.HTTPInputHeader.X-Orginal-HTTPCommand VALUE = NULL;
	  SET OutputRoot.HTTPInputHeader.X-Orginal-HTTPCommand= POST http://127.0.0.1:7800/service/result HTTP/1.1;
		-- CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
